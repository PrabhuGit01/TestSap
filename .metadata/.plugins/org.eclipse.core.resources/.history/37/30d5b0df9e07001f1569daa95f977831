package pages;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.support.PageFactory;


public class JQueryHomePage {

	WebDriver ldriver;

	public JQueryHomePage(WebDriver rDriver)
	{
		ldriver=rDriver;

		PageFactory.initElements(rDriver, this);
	}
	
	
	@FindBy(xpath = "\"//*[@id=\\\"sidebar\\\"]/aside/*[text()='Widgets']/../ul/li//*[text()='Accordion']\"");
	

}
